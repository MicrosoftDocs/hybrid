{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg",
          "**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/hybrid",
      "feedback_product_url": "https://feedback.azure.com/forums/34192--general-feedback",
      "breadcrumb_path": "/hybrid/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "uhfHeaderId": "azure",
      "_op_documentIdPathDepotMapping": {
        "./app-solutions": {
          "depot_name": "Azure.AzureStack",
          "folder_relative_path_in_docset": "hybrid/"
        }
      },
      "manager":"femila",
      "ms.service": "multiple"
    },
      "fileMetadata": {
        "titleSuffix": {
          "**/*.md": "Azure Hybrid Solutions",
          "app-solutions/**/*.md": "Azure Hybrid App Solutions",
          "ops-solutions/**/*.md": "Azure Hybrid Ops Solutions"
        },
        "searchScope": {
          "**/*.md": ["Azure","Hybrid"]
        }
      },
    "template": [],
    "dest": "hybrid",
    "markdownEngineName": "markdig"
  }
}